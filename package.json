{
  "name": "hackernews",
  "version": "0.1.4",
  "private": true,
  "author": "madstone0-0",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.14.6",
    "prop-types": "^15.7.2",
    "react": "^16.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-adsense": "^0.0.6",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.0",
    "react-ga": "^2.5.6",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "build-css": "node-sass --output-style expanded --include-path ./src --include-path ./node_modlues src/ -o src/",
    "watch-css": "npm run build-css && node-sass --output-style expanded --include ./src --include-path: ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava -v",
    "eject": "react-scripts eject",
    "start-js": "react-scripts start",
    "report-coverage": "./node_modules/.bin/nyc report --reporter=lcov > coverage.lcov && codecov",
    "standard": "./node_modules/.bin/standard index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "ava": "^0.16.0",
    "co-exec": "^1.0",
    "codecov": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "ghooks": "^1.2.4",
    "nyc": "^10.0.0",
    "react-test-renderer": "^16.6.3",
    "standard": "^8.4.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run standard"
    }
  }
}
